#include <iostream>
#include<string>
using namespace std;
int main()
{
    string s;
    cin >> s;
    for(int i=0;s[i]!='\0';i++)
    {
        if(islower(s[i]))
        {
            s[i]=toupper(s[i]);
        }
        else if(isupper(s[i]))
        {
            s[i]=tolower(s[i]);
        }
    }
    cout << s << endl;
    return 0;
}
